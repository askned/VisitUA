<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/beforvizit"
    style="@style/Container"
    android:isScrollContainer="true">


    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margincard"
        android:layout_marginBottom="@dimen/marditbott"
        card_view:cardBackgroundColor="@color/backgroundCard"
        card_view:cardCornerRadius="@dimen/cardradius"
        android:id="@+id/view"
        android:clickable="true"
        android:onClick="onClickCalculate">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Рачет стоимости" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="30dp"
            android:layout_marginRight="@dimen/margintop"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"

            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Расчитать сколько будет стоить ваше путишествие" />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margincard"
        android:layout_below="@id/view"
        android:layout_marginBottom="@dimen/marditbott"
        card_view:cardBackgroundColor="@color/backgroundCard"
        card_view:cardCornerRadius="@dimen/cardradius"
        android:id="@+id/view2">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Визовая политика" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="30dp"
            android:layout_marginRight="@dimen/margintop"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"

            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Необходимо ли будет вам оформлять визу перед поездкой" />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margincard"
        android:layout_marginBottom="@dimen/marditbott"
        card_view:cardBackgroundColor="@color/backgroundCard"
        card_view:cardCornerRadius="@dimen/cardradius"
        android:layout_below="@id/view2"
        android:id="@+id/view3"
        android:clickable="true"
        android:onClick="onClickCalculate">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Таможенное оформление" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="30dp"
            android:layout_marginRight="@dimen/margintop"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"

            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Правило ввоза и вывоза на територию Украины" />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margincard"
        android:layout_below="@id/view3"
        android:layout_marginBottom="@dimen/marditbott"
        card_view:cardBackgroundColor="@color/backgroundCard"
        card_view:cardCornerRadius="@dimen/cardradius"
        android:id="@+id/view4">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Иммиграционные правила" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="30dp"
            android:layout_marginRight="@dimen/margintop"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"

            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Правила оформления по прибытию на територию Украины" />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margincard"
        android:layout_marginBottom="@dimen/marditbott"
        card_view:cardBackgroundColor="@color/backgroundCard"
        card_view:cardCornerRadius="@dimen/cardradius"
        android:layout_below="@id/view4"
        android:id="@+id/view5"
        android:clickable="true"
        android:onClick="onClickCalculate">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Средства связи" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="30dp"
            android:layout_marginRight="@dimen/margintop"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"

            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Мобильная связь и интернет в Украине" />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margincard"
        android:layout_below="@id/view5"
        android:layout_marginBottom="@dimen/marditbott"
        card_view:cardBackgroundColor="@color/backgroundCard"
        card_view:cardCornerRadius="@dimen/cardradius"
        android:id="@+id/view6">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="20dp"
            android:textStyle="bold"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Общая информация" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="30dp"
            android:layout_marginRight="@dimen/margintop"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"

            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Общая информация про Украину" />
    </android.support.v7.widget.CardView>
</RelativeLayout>
